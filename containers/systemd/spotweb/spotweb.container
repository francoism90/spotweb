[Unit]
Description=Spotweb container
Requires=spotweb-mariadb.service

[Container]
Image=spotweb.build
AutoUpdate=local
LogDriver=none
UserNS=keep-id
Network=spotweb.network
PublishPort=8085:443
PublishPort=8085:443/udp

[Service]
EnvironmentFile=%h/.config/containers/systemd/spotweb/config/app.env
TimeoutStartSec=90
